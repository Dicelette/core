{
  "name": "@dicelette/core",
  "version": "1.21.2",
  "description": "Core library for the Dicelette Discord bot",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dicelette/core.git"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "build": "tsup",
    "prerelease": "bun run build",
    "lint": "biome format --write src",
    "tsc": "tsc --noEmit --skipLibCheck",
    "release": "commit-and-tag-version",
    "postrelease": "git push --follow-tags origin main && bun publish"
  },
  "keywords": [
    "discord",
    "roll",
    "library",
    "bot",
    "typescript"
  ],
  "author": "Mara-Li",
  "license": "GPL-3.0-only",
  "dependencies": {
    "@dice-roller/rpg-dice-roller": "^5.5.1",
    "mathjs": "^15.1.0",
    "moment": "^2.30.1",
    "random-js": "^2.1.0",
    "remove-accents": "^0.5.0",
    "ts-dedent": "^2.2.0",
    "uniformize": "^2.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.6",
    "@types/bun": "^1.3.2",
    "commit-and-tag-version": "^12.6.0",
    "tslib": "^2.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-json-schema": "^0.65.1",
    "zod-to-json-schema": "^3.24.6"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "esbuild"
  ]
}

{
	"name": "@dicelette/core",
	"version": "1.24.0",
	"description": "Core library for the Dicelette Discord bot",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Dicelette/core.git"
	},
	"type": "commonjs",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"prerelease": "bun run build",
		"lint": "biome check --write src --unsafe",
		"tsc": "tsc --noEmit --skipLibCheck",
		"release": "commit-and-tag-version",
		"postrelease": "git push --follow-tags origin main && bun publish",
		"dev": "tsup --watch"
	},
	"keywords": [
		"discord",
		"roll",
		"library",
		"bot",
		"typescript"
	],
	"author": "Mara-Li",
	"license": "GPL-3.0-only",
	"dependencies": {
		"@dice-roller/rpg-dice-roller": "^5.5.1",
		"mathjs": "^15.1.0",
		"moment": "^2.30.1",
		"random-js": "^2.1.0",
		"remove-accents": "^0.5.0",
		"ts-dedent": "^2.2.0",
		"uniformize": "^2.5.0",
		"vite-tsconfig-paths": "^5.1.4",
		"zod": "^4.1.13"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@types/bun": "^1.3.3",
		"commit-and-tag-version": "^12.6.1",
		"tslib": "^2.8.1",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3",
		"typescript-json-schema": "^0.67.0",
		"zod-to-json-schema": "^3.25.0"
	},
	"trustedDependencies": [
		"@biomejs/biome",
		"esbuild"
	]
}